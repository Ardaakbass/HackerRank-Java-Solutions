import java.io.*;
import java.util.*;

public class Solution {

    /*
     * Complete the 'angryProfessor' function below.
     *
     * The function is expected to return a STRING.
     * The function accepts following parameters:
     * 1. INTEGER k (Threshold)
     * 2. INTEGER_ARRAY a (Arrival times)
     */
    public static String angryProfessor(int k, List<Integer> a) {
        int onTimeStudents = 0;

        // Iterate through arrival times
        for (int time : a) {
            // Check if student arrived on time (time <= 0)
            if (time <= 0) {
                onTimeStudents++;
            }
        }

        // Logic Check: Is class cancelled?
        // If onTimeStudents >= k, class continues (NOT cancelled) -> return "NO"
        // If onTimeStudents < k, class is cancelled -> return "YES"
        if (onTimeStudents >= k) {
            return "NO";
        } else {
            return "YES";
        }
    }

    public static void main(String[] args) {
        // Test Case 1
        List<Integer> a1 = new ArrayList<>(Arrays.asList(-1, -3, 4, 2));
        System.out.println(angryProfessor(3, a1)); // Expected: YES (Only 2 on time, need 3)

        // Test Case 2
        List<Integer> a2 = new ArrayList<>(Arrays.asList(0, -1, 2, 1));
        System.out.println(angryProfessor(2, a2)); // Expected: NO (2 on time, need 2)
    }
}
